@article{Ilanko2015,
abstract = {Field observations have previously identified rapid cyclic changes in the behaviour of the lava lake of Erebus volcano. In order to understand more fully the nature and origins of these cycles, we present here a wavelet-based frequency analysis of time series measurements of gas emissions from the lava lake, obtained by open-path Fourier transform infrared spectroscopy. This reveals (i) a cyclic change in total gas column amount, a likely proxy for gas flux, with a period of about 10 min, and (ii) a similarly phased cyclic change in proportions of volcanic gases, which can be explained in terms of chemical equilibria and pressure-dependent solubilities. Notably, the wavelet analysis shows a persistent periodicity in the CO2/CO ratio and strong periodicity in H2O and SO2 degassing. The ‘peaks' of the cycles, defined by maxima in H2O and SO2 column amounts, coincide with high CO2/CO ratios and proportionally smaller increases in column amounts of CO2, CO, and OCS. We interpret the cycles to arise from recharge of the lake by intermittent pulses of magma from shallow depths, which degas H2O at low pressure, combined with a background gas flux that is decoupled from this very shallow magma degassing.},
author = {Ilanko, Tehnuka and Oppenheimer, Clive and Burgisser, Alain and Kyle, Philip},
doi = {10.1007/s00445-015-0941-z},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Ilanko et al. - 2015 - Cyclic degassing of Erebus volcano, Antarctica.pdf:pdf},
issn = {14320819},
journal = {Bulletin of Volcanology},
keywords = {Antarctica,Degassing,Erebus volcano,Lava lake},
number = {6},
title = {{Cyclic degassing of Erebus volcano, Antarctica}},
volume = {77},
year = {2015}
}
@article{Robert2004,
abstract = {The oscillating flow instability of a molten linear high-density polyethylene$\backslash$nis carefully studied using a single screw extruder equipped with$\backslash$na transparent slit die. Experiments are performed using laser Doppler$\backslash$nvelocimetry in order to obtain the local velocities field across$\backslash$nthe entire die width. At low flow rate, the extrusion is stable and$\backslash$nsteady state velocity profiles are obtained. During the instability,$\backslash$nthe velocity oscillates between two steady state limits, suggesting$\backslash$na periodic stick-slip transition mechanism. At high flow rate, the$\backslash$nflow is mainly characterized by a pronounced wall slip. We show that$\backslash$nwall slip occurs all along the die land. An investigation of the$\backslash$nslip flow conditions shows that wall slip is not homogeneous in a$\backslash$ncross section of the slit die, and that pure plug flow occurs only$\backslash$nfor very high flow rates. A numerical computation of the profile$\backslash$nassuming wall slip boundary conditions is done to obtain the true$\backslash$nlocal wall slip velocity. It confirms that slip velocities are of$\backslash$nthe same order of magnitude as those measured with a capillary rheometer.},
author = {Robert, L. and Demay, Y. and Vergnes, B.},
doi = {10.1007/s00397-003-0323-x},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Robert, Demay, Vergnes - 2004 - Stick-slip flow of high density polyethylene in a transparent slit die investigated by laser Doppler vel.pdf:pdf},
issn = {00354511},
journal = {Rheologica Acta},
keywords = {Extrusion,High density polyethylene,Instability,Laser Doppler velocimetry,Spurt flow,Stick-slip,Wall slip},
number = {1},
pages = {89--98},
title = {{Stick-slip flow of high density polyethylene in a transparent slit die investigated by laser Doppler velocimetry}},
volume = {43},
year = {2004}
}
