@article{Neuberg2000a,
abstract = {Recent observations from Soufriere Hills volcano in Montserrat reveal a wide variety of low-frequency seismic signals. We discuss similarities and differences between hybrid earthquakes and long-period events, and their role in explosions and rockfall events. These events occur usually in swarms, and occasionally merge into tremor, an observation that can shed further light on the generation and composition of harmonic tremor. We use a 2D finite difference method to model major features of low-frequency seismic signatures and compare them with the observations. A depth-dependent velocity model for a fluid-filled conduit is introduced which accounts for the varying gas-content in the magma, and the impact on the seismic signals is discussed. We carefully analyse episodes of tremor that show shifting spectral lines and model those in terms of changes in the gas content of the magma as well as in terms of a time-dependent triggering mechanism of low-frequency resonances. In this way we explain the simultaneous occurrence of low-frequency events and tremor with a spectral content comprising integer harmonics. (C) 2000 Elsevier Science B.V. All rights reserved.},
author = {Neuberg, J. and Luckett, R. and Baptie, B. and Olsen, K.},
