issn = {1343-8832},
journal = {Earth, Planets and Space},
keywords = {Harmonic tremor,degas,infrasound,lava deflation},
month = {jun},
number = {6},
pages = {633--643},
title = {{Switching from seismic to seismo-acoustic harmonic tremor at a transition of eruptive activity during the Shinmoe-dake 2011 eruption}},
url = {http://link.springer.com/10.5047/eps.2013.05.003},
volume = {65},
