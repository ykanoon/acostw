journal = {Journal of Volcanology and Geothermal Research},
keywords = {Classical stratigraphy,Field volcanology,Geological mapping,Volcanic stratigraphy,Volcano geology},
pages = {68--91},
publisher = {Elsevier B.V.},
title = {{Volcanic stratigraphy: A review}},
