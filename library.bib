doi = {10.1029/2008GL035079},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Kumagai et al. - 2008 - Mantle plumes Thin, fat, successful, or failing Constraints to explain hot spot volcanism through time and space.pdf:pdf},
