journal = {J. Geophys. Res.},
keywords = {0394 Atmospheric Composition and Structure: Instru,7280 Seismology: Volcano seismology,8409 Volcanology: Atmospheric effects,8419 Volcanology: Volcano monitoring,8428 Volcanology: Explosive volcanism,Augustine,Kasatochi,eruption source,plume height,tremor},
number = {B1},
pages = {B01306},
title = {{Volcanic plume height measured by seismic waves based on a mechanical model}},
