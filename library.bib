@article{Carr1981,
abstract = {Izalco volcano is a young parasitic cone located 4 km south and seaward from the central crater of Santa Ana volcano, a mature composite cone. At equal SiO2 contents Izalco's lavas have lower large ion lithophile element (LILE) contents than Santa Ana's lavas, but the compositions of Izalco's lavas have converged toward those of Santa Ana with time, possibly by open-system fractionation. Short-term fluctuations in the SiO2 content of lavas at Izalco are closely related to the level of preceding activity; the higher the level of preceding activity, the lower the SiO2 content. A stratigraphic section of lavas from the Santa Ana crater appears to be an inverted magma chamber with a zoned silicic top and a uniform basaltic base. These observations suggest that graded silicic zones are regularly developed at the top of the basaltic magma bodies feeding this volcanic center. Nearly contemporaneous eruptions of distinctly different magma types (1770-1965 at Izalco, 1904, at Santa Ana and 1722 at San Marcellino) suggests the presence of three different magma bodies beneath the Santa Ana complex, a LILE-poor mafic magma body beneath Izalco; a LILE-rich mafic magma body beneath Santa Ana and a rhyolitic magma beneath the Coatepeque caldera, located on the northeast flank of Santa Ana. ?? 1981.},
author = {Carr, Michael J. and Pontier, Nancy K.},
doi = {10.1016/0377-0273(81)90027-5},
