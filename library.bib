title = {{Dome-building eruptions: insights from analogue experiments}},
url = {http://sp.lyellcollection.org/cgi/content/abstract/307/1/207 http://sp.lyellcollection.org/lookup/doi/10.1144/SP307.12},
volume = {307},
year = {2008}
}
