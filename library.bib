keywords = {Strombolian eruptions,acoustic propagation,acoustic source directivity,infrasound,source inversion},
number = {3},
pages = {673--686},
publisher = {Elsevier B.V.},
title = {{Acoustic source characterization of impulsive Strombolian eruptions from the Mount Erebus lava lake}},
url = {http://dx.doi.org/10.1016/j.jvolgeores.2008.06.028},
volume = {177},
