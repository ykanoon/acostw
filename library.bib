pages = {311--328},
title = {{Microgravity change as a precursor to volcanic activity}},
volume = {61},
year = {1994}
}
@article{Johnson2014,
abstract = {Volcan Santiaguito (Guatemala) “breathes” with extraordinary regularity as the edifice's conduit system accumulates free gas, which periodically vents to the atmosphere. Periodic pressurization controls explosion timing, which nearly always occurs at peak inflation, as detected with tiltmeters. Tilt cycles in January 2012 reveal regular 26±6 min inflation/deflation cycles corresponding to at least {\~{}}101 kg/s of gas fluxing the system. Very long period (VLP) earthquakes presage explosions and occur during cycles when inflation rates are most rapid. VLPs locate {\~{}}300mbelow the vent and indicate mobilization of volatiles, which ascend at {\~{}}50 m/s. Rapid gas ascent feeds pyroclast-laden eruptions lasting several minutes and rising to {\~{}}1km. VLPs are not observed during less rapid inflation episodes; instead, gas vents passively through the conduit producing no infrasound and no explosion. These observations intimate that steady gas exsolution and accumulation in shallow reservoirs may drive inflation cycles at open-vent silicic volcanoes.},
author = {Johnson, Jeffrey B. and Lyons, J. J. and Andrews, B. J. and Lees, J. M.},
