isbn = {0377-0273},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {Agglutination,Calatrava,Coalescence,Hawaiian,Lava fountain,Lava-like body,Spatter},
pages = {1--14},
