issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
number = {1-4},
pages = {67--88},
title = {{The Avellino plinian eruption of Somma-Vesuvius (3760 y.B.P.): the progressive evolution from magmatic to hydromagmatic style}},
volume = {58},
year = {1993}
}
