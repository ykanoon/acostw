file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Kim, Koyama, Matsumoto - 2001 - Flow pattern and flow characteristics for counter-current two-phase flow in a vertical round tube with w.pdf:pdf},
isbn = {8192583783},
issn = {03019322},
journal = {International Journal of Multiphase Flow},
keywords = {Counter-current flow,Experiment,Flow pattern,Flow visualization,Slug rise velocity,Two-phase flow,Vertical tube,Void fraction,Wire-coil insert},
number = {12},
pages = {2063--2081},
title = {{Flow pattern and flow characteristics for counter-current two-phase flow in a vertical round tube with wire-coil inserts}},
