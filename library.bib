doi = {10.1130/G38787.1},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Pioli et al. - 2017 - GSA data repository Outgassing and eruption of basaltic magmas The effect of conduit geometry.pdf:pdf},
