issn = {00092509},
journal = {Chemical Engineering Science},
keywords = {Bubble,Fluid mechanics,Multiphase flow,Particle image velocimetry (PIV),Slug flow,Wakes},
number = {22},
pages = {7199--7212},
title = {{Flow in the nose region and annular film around a Taylor bubble rising through vertical column of stagnant and flowing Newtonian fluid}},
volume = {61},
year = {2006}
}
@article{Okumura2014,
abstract = {Repetitive explosions of silicic volcanoes are thought to be triggered by volcanic gases in$\backslash$nthe shallow parts of the conduit. To trap the volcanic gases in the conduit, the lava dome$\backslash$nor plug must be impermeable. However, silicic magma undergoes brittle fracturing during$\backslash$nascent because its viscosity increases via dehydration and groundmass crystallization. Since$\backslash$nfractured magma has high permeability, the reduction of permeability is a key process that$\backslash$nallows gases to accumulate, leading to an explosion. We performed uniaxial compression$\backslash$nexperiments on rhyolite fragments at temperatures of 700–900 °C to investigate the permeability$\backslash$nreduction of fractured rhyolite. Experimental results show that permeability is controlled$\backslash$nby porosity, and permeability reduction can be caused by compaction of fractured$\backslash$nmagma. Based on the observed relationship between permeability and porosity, we calculate$\backslash$nthe time scale of permeability reduction, i.e., the transition from permeable to impermeable$\backslash$nmagma. The calculated time scale (∼100–1000 s) is much shorter than the eruption repose$\backslash$n(∼1–10 h) observed at Sakurajima volcano, Japan. This result implies that an impermeable$\backslash$nlava dome and plug formed during the eruption repose. In contrast, the repose time$\backslash$n(20–40 min) of Santiaguito volcano, Guatemala, is shorter than the time scale of permeability$\backslash$nreduction ({\textgreater}∼10 h); this indicates that gases could not have accumulated before volcanic$\backslash$nexplosions and that other mechanisms could control eruption cyclicity. },
author = {Okumura, Satoshi and Sasaki, Osamu},
doi = {10.1130/G35855.1},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Okumura, Sasaki - 2014 - Permeability reduction of fractured rhyolite in volcanic conduits and its control on eruption cyclicity.pdf:pdf},
isbn = {0091-7613},
issn = {19432682},
journal = {Geology},
number = {10},
pages = {843--846},
title = {{Permeability reduction of fractured rhyolite in volcanic conduits and its control on eruption cyclicity}},
volume = {42},
year = {2014}
}
