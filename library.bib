isbn = {0012-821X},
issn = {0012821X},
journal = {Earth and Planetary Science Letters},
keywords = {Fragmentation process,Infrasonic pulse,Long period seismic event,Miyake-jima},
number = {2},
pages = {291--301},
title = {{Small inflation sources producing seismic and infrasonic pulses during the 2000 eruptions of Miyake-jima, Japan}},
volume = {240},
year = {2005}
}
