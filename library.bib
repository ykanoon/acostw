issn = {00948276},
journal = {Geophysical Research Letters},
pages = {2803--2811},
title = {{The 2007 eruptions and caldera collapse of the Piton de la Fournaise volcano (La R{\'{e}}union Island) from tilt analysis at a single very broadband seismic station Fabrice}},
volume = {41},
year = {2014}
}
