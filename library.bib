keywords = {Active biomonitoring,Metal(loid)s,Volatile elements,Volcanic emissions},
pages = {220--232},
publisher = {Elsevier B.V.},
title = {{Using mosses as biomonitors to study trace element emissions and their distribution in six different volcanic areas}},
