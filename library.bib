keywords = {Bubble deformation,Capillary number,Conduit flow,Strain,Transient state},
pages = {{\#}pagerange{\#}},
publisher = {The Authors},
title = {{Bubble deformation in magma under transient flow conditions ACCEPTED MANUSCRIPT}},
url = {https://ac.els-cdn.com/S0377027318301379/1-s2.0-S0377027318301379-main.pdf?{\_}tid=ef16d8ea-a967-4bbb-89ef-a6feb3b2cfda{\&}acdnat=1537991186{\_}f9447f231b7886e1f392d987e8fb6f0f},
volume = {364},
