keywords = {Real-time collection,Temporal variation,Volcanic ash texture,Vulcanian eruption},
pages = {80--89},
publisher = {Elsevier B.V.},
title = {{Temporal variation in volcanic ash texture during a vulcanian eruption at the sakurajima volcano, Japan}},
url = {http://dx.doi.org/10.1016/j.jvolgeores.2013.05.010},
volume = {260},
year = {2013}
