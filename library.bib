file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Lopez-Escobar, Vergara, Frey - 1981 - Petrology and geochemistry of lavas from Antuco volcano, a basaltic volcano of the Southern Andes.pdf:pdf},
