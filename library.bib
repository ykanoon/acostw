@article{Kozono2012,
abstract = {We investigated the coupled effects of gas escape and crystallization on the dynamics of lava dome eruptions using a one-dimensional conduit flow model. The relationship between chamber pressure pch and mass flow rate q for steady conduit flow commonly has a regime of negative differential resistance (i.e., dpch/dq {\textless} 0), which causes a transition from lava dome to explosive eruption. Two positive-feedback mechanisms that result in negative differential resistance have been identified. First, effective magma viscosity decreases with increasing q because of a delay of crystallization, leading to reduced viscous wall friction (feedback 1). Second, magma porosity increases with increasing q because of less efficient gas escape, leading to reduced gravitational load (feedback 2). For high-phenocryst-content magma (volume fraction {\textgreater}0.5), feedback 1 is the main mechanism that forms negative differential resistance. In this case, the transition from lava dome to explosive eruption occurs when the magma supply rate exceeds a fixed critical value. For low-phenocryst-content magma (volume fraction {\textless}0.5), feedback 2 plays a key role so that the transition is controlled by the permeability of the surrounding rocks; the critical magma supply rate remarkably decreases with decreasing permeability. Transition due to feedback 2 is associated with a change in the chemical composition of volcanic gas, a drastic increase in magma porosity from nearly 0 to greater than 0.8, and overpressure at a shallower level, which can be detected from geochemical and geophysical field observations.},
author = {Kozono, T. and Koyaguchi, T.},
doi = {10.1029/2012JB009343},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Kozono, Koyaguchi - 2012 - Effects of gas escape and crystallization on the complexity of conduit flow dynamics during lava dome eruptio.pdf:pdf},
isbn = {0148-0227},
issn = {21699356},
journal = {Journal of Geophysical Research: Solid Earth},
keywords = {conduit flow model,effect of crystallization on viscosity,lava dome eruption,transition to explosive eruption,vertical and lateral gas escapes},
number = {8},
pages = {1--18},
title = {{Effects of gas escape and crystallization on the complexity of conduit flow dynamics during lava dome eruptions}},
volume = {117},
year = {2012}
}
@article{Johnson2003b,
abstract = {In the most basic seismo-acoustic studies at volcanoes, infrasound monitoring enables differentiation between sub-surface seismicity and the seismicity associated with gas release. Under optimal conditions, complicated degassing signals can be understood, relative explosion size can be assessed, and variable seismo-acoustic energy partitioning can be interpreted. The extent to which these points may be investigated depends upon the quality of the infrasonic records (a function of background wind noise, microphone sensitivity, and microphone array geometry) and the type of activity generated by the volcano (frequency of explosions, bandwidth of the signals, and coupling efficiency of the explosion to elastic energy). To illustrate the features, benefits, and limitations of infrasonic recordings at volcanoes, we showcase acoustic and seismic records from five volcanoes characterized by explosive degassing. These five volcanoes (Erebus in Antarctica, Karymsky in Russia, and Sangay, Tungurahua, and Pichincha in Ecuador) were the focus of seismo-acoustic experiments between 1997 and 2000. Each case study provides background information about the volcanic activity, an overview of visual observations during the period of monitoring, and examples of seismo-acoustic data. We discuss the benefits and utility of the infrasound study at each respective volcano. Finally, we compare the infrasound records and eruptive activity from these volcanoes with other volcanoes that have been the focus of previous seismo-acoustic experiments. ?? 2002 Elsevier Science B.V. All rights reserved.},
author = {Johnson, J. B. and Aster, R. C. and Ruiz, M. C. and Malone, S. D. and McChesney, P. J. and Lees, J. M. and Kyle, P. R.},
doi = {10.1016/S0377-0273(02)00409-2},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Johnson et al. - 2003 - Interpretation and utility of infrasonic records from erupting volcanoes.pdf:pdf},
isbn = {0377-0273},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {Degassing mechanisms,Volcanic infrasound,Volcano monitoring,Volcano seismology},
number = {1-2},
pages = {15--63},
pmid = {126},
title = {{Interpretation and utility of infrasonic records from erupting volcanoes}},
