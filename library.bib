issn = {13438832},
journal = {Earth, Planets and Space},
keywords = {1-d conduit flow model,Bubble growth,Temporal change,Volcano inflation},
number = {12},
pages = {1477--1489},
title = {{Volcano inflation prior to an eruption: Numerical simulations based on a 1-D magma flow model in an open conduit}},
volume = {65},
year = {2013}
}
