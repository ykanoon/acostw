journal = {Journal of Volcanology and Geothermal Research},
keywords = {eruptive stages,hazard and risk,statistics,volcanic eruption},
number = {4},
pages = {275--288},
title = {{Multi-stage volcanic events: A statistical investigation}},
url = {http://www.sciencedirect.com/science/article/B6VCS-4MSHY0C-1/2/c24fe8c940fe73b0e713d4c4fec4f346{\%}5Cnhttp://linkinghub.elsevier.com/retrieve/pii/S0377027306004276},
volume = {161},
