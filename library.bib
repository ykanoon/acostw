@article{Suetani2012,
abstract = {Dripping water from a faucet is a typical example exhibiting rich nonlinear phenomena. For such a system, the time stamps at which water drops separate from the faucet can be directly observed in real experiments, and the time series of intervals $\tau$n between drop separations becomes a subject of analysis. Even if the mass mn of a drop at the onset of the nth separation, which is difficult to observe experimentally, exhibits perfectly deterministic dynamics, it may be difficult to obtain the same information about the underlying dynamics from the time series $\tau$n. This is because the return plot $\tau$n-1 vs. $\tau$n may become a multivalued relation (i.e., it doesn't represent a function describing deterministic dynamics). In this paper, we propose a method to construct a nonlinear coordinate which provides a ``surrogate'' of the internal state mn from the time series of $\tau$n. Here, a key of the proposed approach is to use isomap, which is a well-known method of manifold learning. We first apply it to the time series of $\tau$n generated from the numerical simulation of a phenomenological mass-spring model for the dripping faucet system. It is shown that a clear one-dimensional map is obtained by the proposed approach, whose characteristic quantities such as the Lyapunov exponent, the topological entropy, and the time correlation function coincide with the original dripping faucet system. Furthermore, we also analyze data obtained from real dripping faucet experiments, which also provide promising results.},
author = {Suetani, Hiromichi and Soejima, Karin and Matsuoka, Rei and Parlitz, Ulrich and Hata, Hiroki},
doi = {10.1103/PhysRevE.86.036209},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Suetani et al. - 2012 - Manifold learning approach for chaos in the dripping faucet.pdf:pdf},
isbn = {1539-3755},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
number = {3},
pages = {1--8},
title = {{Manifold learning approach for chaos in the dripping faucet}},
volume = {86},
year = {2012}
}
@article{Bagdassarov1994,
abstract = {The effect of non-deformable inclusions on the frequency-dependent rheology of a rhyolite melt plus crystals has been investigated using a sinusoidal torsion deformation device for measurements of shear viscosity and modulus in the frequency range of 5 mHz to 20 Hz at temperatures of 750-1050-degrees-C. The relaxed shear viscosity and unrelaxed shear modulus of rhyolite magma (rhyolite melt plus crystals plus bubbles) decreases with increasing bubble content and increases with the addition of crystals. At a crystal concentration of about 45{\%} a relaxed value of the shear viscosity is not attainable. The presence of rigid inclusions results in an imaginary component of the shear modulus that becomes more symmetrical and shifted to the low-frequency-high-temperature range with respect to that for a crystal-free melt. The slope of log(Q-1) (internal friction) as a function of the dimensionless variable log(omegatau), is unaffected in the low-temperature-high-frequency range of crystals, with Q-1 almost-equal-to 1/(omegatau)0.5 (the same as for bubble- and crystal-free rhyolite). For the present type of suspension, the internal friction is practically constant and independent of log(omegatau) in the high-temperature-low-frequency limit (omegatau much less than 1). The shape of the Cole-Cole diagram becomes symmetrical and can be described as a Caputo body with parameter gamma almost-equal-to 0.45, whereas for bubble-bearing and inclusion-free rhyolite melts the shape of diagram relates to the beta-relaxation exponent with beta almost-equal-to 0.5. The present work demonstrates that magma may or may not follow a power-law rheology depending on the relative volume proportion between crystals and bubbles.},
author = {Bagdassarov, N.S. and Dingwell, D.B. and Webb, S.L.},
doi = {10.1016/0031-9201(94)90066-3},
file = {:Users/yokanno/Library/Application Support/Mendeley Desktop/Downloaded/Bagdassarov, Dingwell, Webb - 1994 - Viscoelasticity of crystal- and bubble-bearing rhyolite melts.pdf:pdf},
isbn = {0031-9201},
issn = {00319201},
journal = {Physics of the Earth and Planetary Interiors},
number = {2},
pages = {83--99},
title = {{Viscoelasticity of crystal- and bubble-bearing rhyolite melts}},
volume = {83},
year = {1994}
