isbn = {0012-821X},
issn = {0012821X},
journal = {Earth and Planetary Science Letters},
keywords = {Eruptions,Lava,Periodicity,Shield volcanoes},
month = {may},
pages = {173--184},
title = {{Periodic behavior in lava dome eruptions}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0012821X02005575},
volume = {199},
year = {2002}
}
